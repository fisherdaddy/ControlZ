{
    "manifest_version": 3,
    "name": "ControlZ â€“ Easy to undo closed tabs",
    "version": "1.3",
    "description": "Easy to undo closed tabs",
    "permissions": [
      "history",
      "tabs",
      "storage",
      "management"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "settings.html",
    "web_accessible_resources": [
      {
        "resources": [
          "icons/default_favicon.svg"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }
  